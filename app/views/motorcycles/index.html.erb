<%= render 'shared/nav' %>

<br>

<table class="table table-striped table-bordered">
  <thead><th>#</th><th>motorcycle name</th></thead>
  <tbody>
    <% @motorcycles.each_with_index do |m, i| %>
      <tr>
        <td> <%= i + 1 %> </td>
        <td> <%= m.name %> </td>
        <td> <%= link_to 'delete', motorcycle_path(m), method: :delete %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

